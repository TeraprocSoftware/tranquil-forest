.panel.panel-primary
    .panel-heading.text-center Default Test Accounts (uid/pwd = Username/FirstName)
    .panel-body
        .row
            .col-md-2 <u>First Name
            .col-md-2 Last Name
            .col-md-2 Username
            .col-md-2 Email</u>
        .row(ng-repeat="user in users | filter:{enabled:true}")
            .col-md-2 {{user.firstname}}
            .col-md-2 {{user.lastname}}
            .col-md-2 {{user.username}}
            .col-md-2 {{user.email}}

